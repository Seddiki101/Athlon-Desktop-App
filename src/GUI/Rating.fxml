<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import org.controlsfx.control.Rating?>

<AnchorPane id="AnchorPane" prefHeight="227.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.RatingController">
   <children>
      <AnchorPane fx:id="pane" prefHeight="247.0" prefWidth="488.0" style="-fx-background-color: pink;">
         <children>
            <TextField fx:id="userNameTextField" layoutX="132.0" layoutY="37.0" prefHeight="21.0" prefWidth="180.0" promptText="Veuillez saisir votre nom" />
            <Button layoutX="193.0" layoutY="137.0" mnemonicParsing="false" onAction="#addProductRating" text="Noter" />
            <Rating fx:id="ratingFieldStar" layoutX="134.0" layoutY="85.0" rating="0.0">
               <cursor>
                  <Cursor fx:constant="NONE" />
               </cursor>
               <effect>
                  <DropShadow color="#d70101" />
               </effect></Rating>
            <Button fx:id="closeButton" layoutX="445.0" layoutY="22.0" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="16.0" prefWidth="23.0" text="X" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
